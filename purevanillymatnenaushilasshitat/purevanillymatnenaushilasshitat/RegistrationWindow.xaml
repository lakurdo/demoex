<Window x:Class="purevanillymatnenaushilasshitat.RegistrationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:purevanillymatnenaushilasshitat"
        mc:Ignorable="d"
        Title="RegistrationWindow" Height="221" Width="429">
    <Grid Margin="10">
        <StackPanel>
            <TextBlock Text="Логин:"/>
            <TextBox Name="UsernameTextBox" Width="200"/>

            <TextBlock Text="Пароль:" Margin="0,10,0,0"/>
            <PasswordBox Name="PasswordBox" Width="200"/>

            <Button Content="Зарегистрироваться" Width="150" Margin="0,10,0,0" Click="RegisterButton_Click"/>
            <Button Content="Назад" Width="100" Click="BackButton_Click"/>
        </StackPanel>
    </Grid>
</Window>
